		{
			"name": "eliminar-productos",
			"event": [
				{
					"listen": "prerequest",
					"script": {
						"exec": [
							"const secretKey = \"Mercado Libre\";\r",
							"\r",
							"\r",
							"const body = {\r",
							"    \"orden_id\": \"12345\",\r",
							"    \"producto_id\": \"PRO002\",\r",
							"};\r",
							"// Convertir el cuerpo de la solicitud a una cadena JSON.\r",
							"const bodyString = JSON.stringify(body);\r",
							"\r",
							"const bodyAux = JSON.stringify(body).replace(/\\s+/g, '');\r",
							"const hash = CryptoJS.HmacSHA256(bodyString, secretKey).toString(CryptoJS.enc.Hex);\r",
							"\r",
							"\r",
							"pm.environment.set(\"hash\", hash);\r",
							"pm.environment.set(\"body\", bodyString);\r",
							"\r",
							"// Agregar el encabezado X-Signature a la solicitud\r",
							"pm.request.headers.add({ key: \"X-Signature\", value: hash });\r",
							"\r",
							"// ðŸ“‹ Log en consola para depurar\r",
							"console.log(\"Body:\", bodyString);\r",
							"console.log(\"HMAC generado:\", hash);"
						],
